{
  "name": "obj-ui",
  "version": "1.0.0",
  "description": "An npm package for developing user interfaces with object-oriented JavaScript or TypeScript.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "author": "Finley Owen",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "fs": "^0.0.1-security",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finleyowen/obj-ui.git"
  },
  "scripts": {
    "b": "tsc",
    "p": "prettier --write src",
    "l": "tslint -p tsconfig.json",
    "d": "typedoc  --plugin typedoc-plugin-markdown src/*",
    "t": "jest --config jestconfig.json",
    "prepare": "yarn b",
    "prepublishOnly": "yarn t",
    "preversion": "yarn d",
    "version": "yarn p && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.12",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.21"
  }
}
